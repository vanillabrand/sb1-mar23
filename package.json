{
  "type": "module",
  "name": "crypto-trading-strategy",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "NODE_ENV=development node --loader ts-node/esm backend/server.ts",
    "dev:frontend": "vite",
    "start": "bash scripts/run.sh dev",
    "start:prod": "bash scripts/run.sh prod",
    "stop": "bash scripts/run.sh stop",
    "build:frontend": "tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:integration": "jest --config jest.integration.config.ts",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "analyze-bundle": "vite build --mode analyze",
    "validate": "bash scripts/pre-deploy-check.sh"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@tanstack/react-query": "^5.69.0",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "framer-motion": "^12.6.2",
    "lucide-react": "^0.484.0",
    "papaparse": "^5.5.2",
    "react-router-dom": "^6.30.0",
    "recharts": "^2.15.1",
    "use-sound": "^5.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@mswjs/data": "^0.16.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.27",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.7.3",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.17",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "^2.0.0",
    "jsdom": {
      "domexception": "^4.0.0",
      "abab": "^2.0.6"
    }
  }
}
